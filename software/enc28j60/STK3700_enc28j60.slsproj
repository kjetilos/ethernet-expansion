    <!--exclude pattern=".*/heap_1.c" /-->
    <exclude pattern=".*/heap_2.c" />
    <exclude pattern=".*/heap_3.c" />
    <exclude pattern=".*/heap_4.c" />
    <exclude pattern=".*/heap_5.c" />
  </module>

  <includePath uri="lwIP/lwip/src/include" />
